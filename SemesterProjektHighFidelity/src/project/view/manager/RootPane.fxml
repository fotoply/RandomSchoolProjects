<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="500.0" prefWidth="700.0"
            xmlns="http://javafx.com/javafx/8.0.40" fx:controller="project.view.manager.RootPaneController">
    <children>
        <ImageView fitHeight="500.0" fitWidth="800.0" pickOnBounds="true" AnchorPane.bottomAnchor="0.0"
